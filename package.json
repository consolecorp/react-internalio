{
  "name": "react-internalio",
  "version": "0.1.5",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:consolecorp/react-internalio.git"
  },
  "keywords": [
    "react",
    "react-component",
    "component",
    "internalio",
    "embed-internalio",
    "internal-tools"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "peerDependencies": {
    "react": ">=16.0.1",
    "react-dom": ">=16.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:npm": "webpack --config npm.config.js --mode production",
    "eject": "react-scripts eject",
    "clean-type-defs": "rm App.d.ts && rm App.d.ts.map && rm index.d.ts && rm index.d.ts.map",
    "cp-static-files": "cp ./package.json ./dist/package.json && cp ./README.md ./dist/README.md",
    "publish:npm": "rm -rf dist && mkdir dist && yarn build:npm && yarn cp-static-files && cd dist && yarn clean-type-defs && npm publish --registry https://registry.npmjs.org"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react": ">=18.0.1",
    "react-dom": ">=18.0.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "cra-template-typescript": "1.2.0",
    "webpack-cli": "^4.10.0",
    "react-scripts": "4.0.3",
    "ts-loader": "^8.2.1",
    "typescript": "^4.8.4"
  }
}
